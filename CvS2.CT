-- Gets the specified hitbox coordinates
function get_obj_boxes(sender)
    if selectedObj == nil or sender == nil or game.base == 0 then return end
    local debug = false
    local copyingCLSN = false
    local copyingPush = false
    if sender.Caption ~= nil and sender.Caption:match("Copy CLSNs") then
        copyingCLSN = true
    end
    if sender.Caption ~= nil and sender.Caption:match("Copy Push") then
        copyingPush = true
    end
    local baseAddr = game.base + selectedObj.addr
    local headBoxTable = readInteger(baseAddr + game.vulnStart + 0x0)
    local bodyBoxTable = readInteger(baseAddr + game.vulnStart + 0x4)
    local legsBoxTable = readInteger(baseAddr + game.vulnStart + 0x8)
    local pushBoxTable = readInteger(baseAddr + game.vulnStart + 0xC)
    local atckBoxTable = readInteger(baseAddr + game.atckStart)
    local boxOffsTable = 0x20000000 + readInteger(baseAddr + game.offset + 0x198)
    local thrOffsTable = 0x20000000 + readInteger(baseAddr + game.offset + 0x19C)
    local animPtr = readInteger(baseAddr + game.anim)

    -- Head box
    local headBoxOffset = readByte(boxOffsTable + 0)
    local hbOff = bShl(headBoxOffset, game.shift)

    -- Body box
    local bodyBoxOffset = readByte(boxOffsTable + 1)
    local bbOff = bShl(bodyBoxOffset, game.shift)

    -- Legs box
    local legsBoxOffset = readByte(boxOffsTable + 2)
    local lbOff = bShl(legsBoxOffset, game.shift)

    -- Push box
    local pushBoxOffset = readByte(boxOffsTable + 4)
    local pbOff = bShl(pushBoxOffset, game.shift)

    -- Sprite number
    local spriteNo = readShort(baseAddr + game.offset + game.sprite)

    -- Debug
    if debug then
        print(string.format("boxOffsTable = 0x%X", boxOffsTable))
        print(string.format("headBoxTable = 0x%X", headBoxTable))
        print("headBoxOffset = " .. headBoxOffset)
        print("bodyBoxOffset = " .. bodyBoxOffset)
        print("legsBoxOffset = " .. legsBoxOffset)
    end

    -- Load the damn pushbox
    selectedObj[HITBOX_PUSH] = {}
    hitbox_load(selectedObj, 0, HITBOX_PUSH, pushBoxTable + pbOff)

    -- Load the damn hitboxes
    selectedObj[HITBOX_VULNERABILITY] = {}
    hitbox_load(selectedObj, 0, HITBOX_VULNERABILITY, headBoxTable + hbOff)
    hitbox_load(selectedObj, 1, HITBOX_VULNERABILITY, bodyBoxTable + bbOff)
    hitbox_load(selectedObj, 2, HITBOX_VULNERABILITY, legsBoxTable + lbOff)


    -- Are we on a new frame?
    if get_frames_drawn() ~= currFrame then
        drawThrow = false
    end
    -- Load the damn attack box
    if not drawThrow then
        selectedObj[HITBOX_ATTACK] = {}
        local atckBoxOffset = animPtr + game.atckID
        local abOff = readByte(base + atckBoxOffset)
        if abOff then
            hitbox_load(selectedObj, 0, HITBOX_ATTACK, atckBoxTable + abOff*0x20)
        end
        --hitbox_load(selectedObj, 0, HITBOX_ATTACK, atckBoxTable + readByte(0x203F1FCC)*0x20) -- only works for p1
    end

    -- Print them
    local clsn2 = convert_to_air(selectedObj, HITBOX_VULNERABILITY)
    local clsn1 = convert_to_air(selectedObj, HITBOX_ATTACK)
    local clsn3 = convert_to_air(selectedObj, HITBOX_PUSH)

    -- Copying to clipboard?
    if copyingCLSN then
        if clsn1 == "" then
            writeToClipboard(clsn2)
        else
            writeToClipboard(clsn2 .. "\n" .. clsn1)
        end
    elseif copyingPush and clsn3 ~= "" then
        writeToClipboard(clsn3)
    end

    -- Logging animations?
    if logAnimations then
        if currFrame ~= get_frames_drawn() then
            currFrameCount = currFrameCount + 1
        end
        if spriteNo ~= lastSprite or
        not table_eq(selectedObj[HITBOX_PUSH], lastObj[HITBOX_PUSH]) or
        not table_eq(selectedObj[HITBOX_ATTACK], lastObj[HITBOX_ATTACK]) or
        not table_eq(selectedObj[HITBOX_VULNERABILITY], lastObj[HITBOX_VULNERABILITY]) then
            lastClsn2 = convert_to_air(lastObj, HITBOX_VULNERABILITY)
            lastClsn1 = convert_to_air(lastObj, HITBOX_ATTACK)
            lastClsn3 = convert_to_air(lastObj, HITBOX_PUSH)

            if currFrameCount &gt; 0 then
                -- Print the current hitboxes.
                if lastClsn1 == "" then
                    logFile:write(lastClsn2 .. "\n")
                else
                    logFile:write(lastClsn2 .. "\n" .. lastClsn1 .. "\n")
                end

                -- Print the current spriteNo and timing
                logFile:write(string.format("0,%d, 0,0, %d\n", lastSprite, currFrameCount))
            end

            -- Reset the data
            lastObj = deepcopy(selectedObj)
            lastSprite = spriteNo

            -- Reset the frame count.
            currFrameCount = 1
        else
            currFrameCount = currFrameCount + 1
        end
    end
end

-- Converts hitboxes to .AIR syntax
function convert_to_air(obj, type)
    local boxes = obj[type]
    local clsnBase = ""
    local clsnStr = ""
    local clsnCount = count(boxes)

    -- If there are no CLSNs, return an empty string
    if clsnCount == 0 then
        return ""
    end

    -- There can be only one attack box at any given moment
    if type == HITBOX_ATTACK then
        clsnCount = 1
    end

    if type == HITBOX_VULNERABILITY then
        clsnBase = "Clsn2"
    elseif type == HITBOX_ATTACK then
        clsnBase = "Clsn1"
    else
        clsnBase = "Clsn3"
    end

    clsnsStr = ""
    boxNo = 0
    for i=0,clsnCount-1 do
        local box
        if type == HITBOX_VULNERABILITY then box = boxes[i] else box = boxes end
        if box ~= nil then
            local coords = ""
            if box.left == 0 and box.top == 0 and box.right == 0 and box.bottom == 0 then
                clsnCount = clsnCount - 1
            else
                if not scaleCLSN then
                    coords = string.format("%d,%d,%d,%d", box.left, box.top, box.right, box.bottom)
                else
                    coords = string.format("%d,%d,%d,%d", tointeger((box.left*5)/6), box.top, tointeger((box.right*5)/6), box.bottom)
                end
                clsnsStr = clsnsStr .. string.format("\n  %s[%d] = ", clsnBase, boxNo) .. coords
                boxNo = boxNo + 1
            end
        end
    end

    if clsnCount == 0 then
        return ""
    else
        clsnStr = clsnBase .. ": " .. clsnCount
        return clsnStr .. clsnsStr
    end
end

-- Swaps 16-bit words.
function word_swap(input)
    local front = bAnd(input, 0xFFFF)
    local back = bAnd(input, 0xFFFF0000)
    local newBack = bShl(front, 16)
    local newFront = bShr(back, 16)
    return bOr(newFront, newBack)
end

-- Thanks, Dammit!
function hitbox_load(obj, i, type, addr)
    local addrBase = 0x20000000 + addr
	local hval    = readShort(addrBase + 0, true)
	local vval    = readShort(addrBase + 2, true)
	local hrad    = readShort(addrBase + 4, true)
	local vrad    = readShort(addrBase + 6, true)
    --local debug = string.format("hval = 0x%X, vval = 0x%X, hrad = 0x%X, vrad = 0x%X", hval,vval,hrad,vrad)
    --print(string.format("addr = 0x%X", addr))
    --print(string.format("addrBase = 0x%X", addrBase))
    --print(debug)

	local left   = - (hval + hrad)
	local top    = - (vval + vrad)
	local right  = - (hval - hrad)
	local bottom = - (vval - vrad)

	if type == HITBOX_VULNERABILITY then
		obj[HITBOX_VULNERABILITY][i] = {
			left   = left,
			right  = right,
			bottom = bottom,
			top    = top,
			hval   = hval,
			vval   = vval,
			type   = type
		}
	else
		obj[type] = {
			left   = left,
			right  = right,
			bottom = bottom,
			top    = top,
			hval   = hval,
			vval   = vval,
			type   = type
		}
	end
end

-- Populate the listbox
function initialize_data(listbox)
    dataInitialized = true
    local listItems = {}
    listbox.items.clear()
    --print("AAAAAAAAAAGH")
    objects = {player1, player2}
    for i=0,game.numProj do
        objects[i+3] = {name = string.format("Projectile %d", i),
                        addr = game.projStart+(game.projOffset*i)
        }
    end
    for k,v in spairs(objects, compare_numeric_names) do
        listbox.items.add(objects[k]['name'])
    end
    selectedObj = objects[0]
end

function set_selected_obj(sender, user)
    for _,v in pairs(objects) do
        if sender.Items[sender.getItemIndex()]:match(v['name']) then
            selectedObj = v
            break
        end
    end
    --print(string.format("Selected obj addr: 0x%X", game.base + selectedObj.addr))
end

-- Pauses the game
function pause_unpause(sender)
    local pauseAddr = 0
    if mode &gt; CVS2_JP then pauseAddr = game.base + 0x1CF90 else pauseAddr = game.base + 0x97AD0 end
    local isPaused = readByte(pauseAddr)
    if isPaused == 0 then
        writeBytes(pauseAddr, 3)
    else
        writeBytes(pauseAddr, 0)
    end
end

-- Returns the number of frames drawn so far
function get_frames_drawn()
    local addr = 0
    if mode &gt; CVS2_JP then
        addr = game.base + 0x1CCCC
    else
        addr = game.base + 0x1D38C
    end
    return readInteger(addr)
end

-- Steps the emulator forward by 1 frame
function step_frame(sender)
    local pauseAddr = 0
    if mode &gt; CVS2_JP then
        pauseAddr = game.base + 0x1CF90
    else
        pauseAddr = game.base + 0x97AD0
    end
    writeInteger(pauseAddr, 3)
    local currentFrame = get_frames_drawn()
    writeInteger(pauseAddr, 0)
    while get_frames_drawn() == currentFrame do
    end
    writeInteger(pauseAddr, 3)
end

function draw_hitbox(axis, box, canvas)
    if axis == nil or box == nil or canvas == nil then return end
    if axis.x == nil or axis.y == nil or box.left == nil or box.right == nil or box.top == nil or box.bottom == nil then return end
    local left   = axis.x + box.left--*(640/384)
    local right  = axis.x + box.right--*(640/384)
    local top    = axis.y + box.top--*(480/224)
    local bottom = axis.y + box.bottom--*(480/224)

    local color = box.type == HITBOX_ATTACK and 0x0000FF or box.type == HITBOX_PUSH and 0x00FF00 or 0xFF0000

    draw_rect(canvas, left,top, right,bottom, color)
end

function draw_rect(canvas, left, top, right, bottom, color)
    canvas:getPen():setColor(color)
    canvas:getPen():setWidth(1)
    canvas:line(left,top, right,top)
    canvas:line(left,top, left,bottom)
    canvas:line(right,top, right,bottom+1)
    canvas:line(left,bottom, right,bottom)
end

-- Displays the hitboxes in the window
function display_hitboxes(form)
    if form == nil or selectedObj == nil then return end
    local c = form.getCanvas()
    c:clear()

    -- Canvas centers
    local posXInt = readShort(game.base + selectedObj.addr + game.posX, true)
    local posYInt = readShort(game.base + selectedObj.addr + game.posY, true)
    posX = posXInt + (readShort(game.base + selectedObj.addr + game.posXFlt, false) / 65536.0)
    posY = - (posYInt + (readShort(game.base + selectedObj.addr + game.posYFlt, false) / 65536.0))
    local centerX = c.getWidth()/2.0--(11.0*(posXInt-32)/6.0) + 34
    local centerY = c:getHeight()/2.0 --posY

    -- Canvas offsets
    local offsetX = 0---(160 - readShort(game.base+game.cameraX, true))---128
    local offsetY = 50--441-readShort(game.base+game.cameraY, true)--64

    local center = {x = centerX+offsetX, y = centerY+offsetY}

    -- Draw axes
    c:getPen():setColor(0x000000)
    c:getPen():setWidth(1)
    c:line(centerX+offsetX-8,centerY+offsetY, centerX+offsetX+9,centerY+offsetY)
    c:line(centerX+offsetX,centerY+offsetY-8, centerX+offsetX,centerY+offsetY+10)

    -- Do nothing if nothing is selected
    if selectedObj == nil then return end

    get_obj_boxes(form)

    local boxes = selectedObj[HITBOX_VULNERABILITY]
    local clsnCount = count(boxes)

    -- Draw the damn push box
    box = selectedObj[HITBOX_PUSH]
    if box ~= nil then
        draw_hitbox(center, box, c)
    end

    -- Draw the damn vulnerability boxes
    for i=0,clsnCount-1 do
        box = boxes[i]
        if box ~= nil and next(box) ~= nil then
            draw_hitbox(center, box, c)
        end
    end

    -- Draw the damn attack box
    box = selectedObj[HITBOX_ATTACK]
    if box ~= nil then
        draw_hitbox(center, box, c)
    end

    return clsnStr
end

-- If checked, will scale horizontal CLSNs
function scale_clsns(sender)
    if checkbox_getState(sender) == 0 then
        scaleCLSN = false
    else
        scaleCLSN = true
    end
end

-- Sets logging to the opposite value
function set_logging(sender)
    if sender:getState() == 0 then
        -- Close the log file if it's open.
        if logFile ~= nil then
            logFile:close()
        end
        logAnimations = false
    end
    if sender:getState() == 1 then
        -- Open the log file for writing.
        logFile = assert(io.open(ANIM_LOG_NAME, "w"))
        logAnimations = true
    end
end
