-- Gets the specified hitbox coordinates
function get_obj_data(sender, obj)
    if obj == nil or game.base == 0 then return end
    local debugPrint = false
    local copyingCLSN = false
    local copyingPush = false
    if sender ~= nil then
        if sender.Caption ~= nil and sender.Caption:match("Copy CLSNs") then
            copyingCLSN = true
        end
        if sender.Caption ~= nil and sender.Caption:match("Copy Push") then
            copyingPush = true
        end
    end
    local baseAddr = game.base + obj.addr
    local headBoxTable = readInteger(baseAddr + game.vulnStart + 0x0)
    local bodyBoxTable = readInteger(baseAddr + game.vulnStart + 0x4)
    local legsBoxTable = readInteger(baseAddr + game.vulnStart + 0x8)
    local pushBoxTable = readInteger(baseAddr + game.vulnStart + 0xC)
    local atckBoxTable = readInteger(baseAddr + game.atckStart)
    local boxOffsTable = 0x20000000 + readInteger(baseAddr + game.offset + 0x198)
    local thrOffsTable = 0x20000000 + readInteger(baseAddr + game.offset + 0x19C)
    local animPtr = readInteger(baseAddr + game.anim)

    -- Update the velocities
    if isActive(obj) then
        obj.velXInt = readShort(game.base + obj.addr + game.velX, true)
        obj.velYInt = readShort(game.base + obj.addr + game.velY, true)
        obj.velX = obj.velXInt + (readShort(game.base + obj.addr + game.velXFlt, false) / 65536.0)
        obj.velY = - (obj.velYInt + (readShort(game.base + obj.addr + game.velYFlt, false) / 65536.0))

        -- Update the positions
        obj.posXInt = readShort(game.base + obj.addr + game.posX, true)
        obj.posYInt = readShort(game.base + obj.addr + game.posY, true)
        obj.posX = obj.posXInt + (readShort(game.base + obj.addr + game.posXFlt, false) / 65536.0)
        obj.posY = - (obj.posYInt + (readShort(game.base + obj.addr + game.posYFlt, false) / 65536.0))

        -- Deltas
        if obj.prevVelX ~= nil then
            obj.deltaVelX = obj.velX - obj.prevVelX
            obj.deltaVelY = obj.velY - obj.prevVelY
        else
            obj.deltaVelX = 0
            obj.deltaVelY = 0
            obj.prevVelX = 0
            obj.prevVelY = 0
        end

        -- Previous velocities
        obj.prevVelX = obj.velX
        obj.prevVelY = obj.velY

        -- Head box
        local headBoxOffset = readByte(boxOffsTable + 0)
        local hbOff = bShl(headBoxOffset, game.shift)

        -- Body box
        local bodyBoxOffset = readByte(boxOffsTable + 1)
        local bbOff = bShl(bodyBoxOffset, game.shift)

        -- Legs box
        local legsBoxOffset = readByte(boxOffsTable + 2)
        local lbOff = bShl(legsBoxOffset, game.shift)

        -- Push box
        local pushBoxOffset = readByte(boxOffsTable + 4)
        local pbOff = bShl(pushBoxOffset, game.shift)

        -- Sprite number
        local spriteNo = readShort(game.base + obj.addr + game.sprite)
        obj[SPRITE_NO] = spriteNo

        -- Debug
        if debugPrint then
          print(string.format("boxOffsTable = 0x%X", boxOffsTable))
          print(string.format("headBoxTable = 0x%X", headBoxTable))
          print("spriteNo = " .. spriteNo)
          print("headBoxOffset = " .. headBoxOffset)
          print("bodyBoxOffset = " .. bodyBoxOffset)
          print("legsBoxOffset = " .. legsBoxOffset)
        end

        -- Load the damn pushbox
        obj[HITBOX_PUSH] = {}
        hitbox_load(obj, 0, HITBOX_PUSH, pushBoxTable + pbOff)

        -- Load the damn hitboxes
        obj[HITBOX_VULNERABILITY] = {}
        hitbox_load(obj, 0, HITBOX_VULNERABILITY, headBoxTable + hbOff)
        hitbox_load(obj, 1, HITBOX_VULNERABILITY, bodyBoxTable + bbOff)
        hitbox_load(obj, 2, HITBOX_VULNERABILITY, legsBoxTable + lbOff)

        -- Are we on a new frame?
        if get_frames_drawn() ~= currFrame and didDrawThrow then
          drawThrow = false
        end
        -- Load the damn attack box
        if not drawThrow then
          obj[HITBOX_ATTACK] = {}
          local atckBoxOffset = animPtr + game.atckID
          local abOff = readByte(base + atckBoxOffset)
          if abOff then
            if debugPrint then print("abOff = " .. abOff) end
            hitbox_load(obj, 0, HITBOX_ATTACK, atckBoxTable + abOff*0x20)
          end
        end

        -- Print them
        local clsn2 = convert_to_air(selectedObj, HITBOX_VULNERABILITY)
        local clsn1 = convert_to_air(selectedObj, HITBOX_ATTACK)
        local clsn0 = convert_to_air(selectedObj, HITBOX_PUSH)

        -- Copying to clipboard?
        if copyingCLSN then
            if clsn1 == "" then
                writeToClipboard(clsn2)
            else
                writeToClipboard(clsn2 .. "\n" .. clsn1)
            end
        elseif copyingPush and clsn0 ~= "" then
            writeToClipboard(clsn0)
        end

        if drawThrow then
           didDrawThrow = true
        end

        -- Logging animations?
        if logAnimations and obj == selectedObj then
            if lastObj == nil or
            selectedObj[SPRITE_NO] ~= lastObj[SPRITE_NO] or
            not table_eq(selectedObj[HITBOX_PUSH], lastObj[HITBOX_PUSH]) or
            not table_eq(selectedObj[HITBOX_ATTACK], lastObj[HITBOX_ATTACK]) or
            not table_eq(selectedObj[HITBOX_VULNERABILITY], lastObj[HITBOX_VULNERABILITY]) then
                if lastObj ~= nil then
                    local lastClsn2 = convert_to_air(lastObj, HITBOX_VULNERABILITY)
                    local lastClsn1 = convert_to_air(lastObj, HITBOX_ATTACK)
                    local lastClsn0 = convert_to_air(lastObj, HITBOX_PUSH)
                    local lastSprite = lastObj[SPRITE_NO]
                    local lastFrameCount = lastObj[FRAME_COUNT]

                    if lastFrameCount and lastFrameCount &gt; 0 then
                        -- Print the current hitboxes.
                        if lastClsn1 == "" then
                            logFile:write(lastClsn2 .. "\n")
                        else
                            logFile:write(lastClsn2 .. "\n" .. lastClsn1 .. "\n")
                        end

                        -- Print the current spriteNo and timing
                        logFile:write(string.format("0,%d, 0,0, %d\n", lastSprite, lastFrameCount))
                    end
                end

                -- Reset the data
                lastObj = deepcopy(selectedObj)

                -- Reset the frame count.
                selectedObj[FRAME_COUNT] = 1
            end
        end
    end
end

function sign(int)
    return int == 0 and 0 or (int &lt; 0 and -1 or 1)
end

-- Converts hitboxes to .AIR syntax
function convert_to_air(obj, type)
    if obj == nil then return "" end

    local boxes = obj[type]
    local clsnBase = ""
    local clsnStr = ""
    local clsnCount = count(boxes)

    -- If there are no CLSNs, return an empty string
    if clsnCount == 0 then
        return ""
    end

    -- There can be only one attack box at any given moment
    if type == HITBOX_ATTACK then
        clsnCount = 1
    end

    if type == HITBOX_VULNERABILITY then
        clsnBase = "Clsn2"
    elseif type == HITBOX_ATTACK then
        clsnBase = "Clsn1"
    else
        clsnBase = "Clsn0"
    end

    clsnsStr = ""
    boxNo = 0
    for i=0,clsnCount-1 do
        local box
        if type == HITBOX_VULNERABILITY then box = boxes[i] else box = boxes end
        if box ~= nil then
            local coords = ""
            if box.left == 0 and box.top == 0 and box.right == 0 and box.bottom == 0 then
                clsnCount = clsnCount - 1
            else
                if not scaleCLSN then
                    coords = string.format("%d,%d,%d,%d", box.left, box.top, box.right, box.bottom)
                else
                    if cfjMode then
                        coords = string.format("%d,%d,%d,%d", tointeger((box.left*5)/6), box.top, tointeger((box.right*5)/6), box.bottom)
                    else
                        local doXScale = (readByte(game.base + obj.addr + game.scaleX)&amp;1) == 1
                        if doXScale then
                            coords = string.format("%d,%d,%d,%d", tointeger((box.left*160.0*6.0)/(179.0*5.0) + sign(box.left)), box.top, tointeger((box.right*160.0*6.0)/(179.0*5.0) + sign(box.right)), box.bottom)
                        else
                            coords = string.format("%d,%d,%d,%d", tointeger((box.left*160.0)/179.0), box.top, tointeger((box.right*160.0)/179.0), box.bottom)
                        end
                    end
                end
                clsnsStr = clsnsStr .. string.format("\n  %s[%d] = ", clsnBase, boxNo) .. coords
                boxNo = boxNo + 1
            end
        end
    end

    if clsnCount == 0 then
        return ""
    else
        clsnStr = clsnBase .. ": " .. clsnCount
        return clsnStr .. clsnsStr
    end
end

-- Swaps 16-bit words.
function word_swap(input)
    local front = bAnd(input, 0xFFFF)
    local back = bAnd(input, 0xFFFF0000)
    local newBack = bShl(front, 16)
    local newFront = bShr(back, 16)
    return bOr(newFront, newBack)
end

-- Thanks, Dammit!
function hitbox_load(obj, i, type, addr)
    local addrBase = 0x20000000 + addr
	local hval    = readShort(addrBase + 0, true)
	local vval    = readShort(addrBase + 2, true)
	local hrad    = readShort(addrBase + 4, true)
	local vrad    = readShort(addrBase + 6, true)
    --local debug = string.format("hval = 0x%X, vval = 0x%X, hrad = 0x%X, vrad = 0x%X", hval,vval,hrad,vrad)
    --print(string.format("addr = 0x%X", addr))
    --print(string.format("addrBase = 0x%X", addrBase))
    --print(debug)

	local left   = -(hval + hrad)
	local top    = -(vval + vrad)
	local right  = -(hval - hrad)
	local bottom = -(vval - vrad)

	if type == HITBOX_VULNERABILITY then
		obj[HITBOX_VULNERABILITY][i] = {
			left   = left,
			right  = right,
			bottom = bottom,
			top    = top,
			hval   = hval,
			vval   = vval,
			type   = type
		}
	else
		obj[type] = {
			left   = left,
			right  = right,
			bottom = bottom,
			top    = top,
			hval   = hval,
			vval   = vval,
			type   = type
		}
	end
end

-- Populate the listbox
function initialize_data(listbox)
    local listItems = {}
    listbox.items.clear()
    objects = {player1, player2}
    for i=0,game.numProj do
        objects[i+3] = {name = string.format("Projectile %d", i),
                        addr = game.projStart+(game.projOffset*i),
                        boxSprites = {}
        }
    end
    for _,obj in pairs(objects) do
        obj.axisSpr = emuScreen.createSprite(texC)
        axisSize.width = obj.axisSpr.Width
        axisSize.height = obj.axisSpr.Height
        obj.boxSprites[HITBOX_VULNERABILITY] = {}
        for i=0,2 do
            obj.boxSprites[HITBOX_VULNERABILITY][i] = emuScreen.createSprite(texV)
        end
        obj.boxSprites[HITBOX_PUSH] = emuScreen.createSprite(texP)
        obj.boxSprites[HITBOX_ATTACK] = emuScreen.createSprite(texA)
    end
    for k,v in spairs(objects, compare_numeric_names) do
        listbox.items.add(objects[k]['name'])
    end
    selectedObj = objects[1]
    dataInitialized = true
end

