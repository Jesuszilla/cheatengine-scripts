        base = boxBase.r5875
    elseif getProcessIDFromProcessName("pcsx2.exe") == processid then
        base = boxBase.pcsx2
    -- DEmul hitbox base
    elseif getProcessIDFromProcessName("demul.exe") == processid then
        base = boxBase.demul
        game.base = emuBase.demul
        mode = CVS2_DC
        player2.addr = 0x1D34
        game.projStart = -0x18E2
        game.projOffset = 0x184
    end

    -- Create the direct3D hook
    if emuScreen == nil then
        emuScreen = createD3DHook()
        --print(emuScreen.Width)
        --print(emuScreen.Height)
        -- Create the images we need
        if vulnPic == nil then
            vulnPic = createPicture()
            vulnPic.loadFromStream(findTableFile("vuln.bmp").Stream)
            atckPic = createPicture()
            atckPic.loadFromStream(findTableFile("atck.bmp").Stream)
            pushPic = createPicture()
            pushPic.loadFromStream(findTableFile("push.bmp").Stream)
            axisPic = createPicture()
            axisPic.loadFromStream(findTableFile("axis.png").Stream)
            texV = emuScreen.createTexture(vulnPic)
            texP = emuScreen.createTexture(pushPic)
            texA = emuScreen.createTexture(atckPic)
            texC = emuScreen.createTexture(axisPic)
        end
    end
end

-- Updates the velocity boxes
function update_data(Thread, form)
    if getOpenedProcessID() &gt; 0 then
        synchronize(timer_setEnabled, form.findComponentByName("Start"), false)
        while not terminateThread do
            if form == nil then return end
            -- Update the selected object's data if everything's been initialized.
            -- Initialize the data if not already done so.
            if not dataInitialized then
                debugProcess()
                check_name(form)
                local objList = form.findComponentByName("ObjectListBox")
                initialize_data(objList)
                check_breakpoints(nil)
                crackHeadTimer = createTimer(form)
                timer_setInterval(crackHeadTimer, 15)

            else
                synchronize(check_proj, form)
                synchronize(update_obj_data, form)
                display_hitboxes()
            end
            nextFrame = get_frames_drawn_60()
            while nextFrame == currFrame60 do nextFrame = get_frames_drawn_60() end
            currFrame60 = nextFrame
            if logAnimations and crackHeadTimer ~= nil and crackHeadTimer.OnTimer == nil then
                synchronize(timer_onTimer, crackHeadTimer, increment_frame_count)
            end
        end
    end
end

