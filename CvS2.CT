-- Pauses the game
function pause_unpause(sender)
    local pauseAddr = 0
    if mode &gt; CVS2_JP then pauseAddr = game.base + 0x1CF90 else pauseAddr = game.base + 0x97AD0 end
    local isPaused = readByte(pauseAddr)
    if isPaused == 0 then
        writeBytes(pauseAddr, 3)
    else
        writeBytes(pauseAddr, 0)
    end
end

function get_frames_drawn_60()
    local addr = 0
    if mode == CVS2_DC then
        addr = game.base - 0x1A818
    else
        addr = addressList.getMemoryRecordByDescription("60FPS Timer (wow64)").getAddress()--0x036D6134--0x01E447A4
    end
    local res = readInteger(addr)
    return res
end

-- Returns the number of frames drawn so far
function get_frames_drawn()
    local addr = 0
    if mode &gt; CVS2_JP then
        addr = game.base + 0x1CCCC
    elseif mode &gt; CVS2_DC then
        addr = game.base + 0x1D38C
    else
        addr = game.base - 0x1A818
    end
    return readInteger(addr)
end

-- Steps the emulator forward by 1 frame
function step_frame(sender)
    local pauseAddr = 0
    if mode &gt; CVS2_JP then
        pauseAddr = game.base + 0x1CF90
    else
        pauseAddr = game.base + 0x97AD0
    end
    writeInteger(pauseAddr, 3)
    local currentFrame = get_frames_drawn()
    writeInteger(pauseAddr, 0)
    while get_frames_drawn() == currentFrame do
    end
    writeInteger(pauseAddr, 3)
end

function draw_hitbox(spr, obj, box, offsetX, offsetY, alpha)
    if box == nil then return end
    if box.left == nil or box.right == nil or box.top == nil or box.bottom == nil then return end
    if spr ~= nil then
        flip = readByte(game.base + obj.addr + game.flip)
        width = math.ceil((box.right - box.left + 1)*posXScale)
        height = math.ceil((1 + math.abs(box.top - box.bottom))*yScale)
        -- Now do the sprite crap
        spr.Alphablend = alpha

        -- DO THE FLIP
        if flip == 1 then
           spr.X = (180 + offsetCFJ - offsetX + obj.posXInt)*posXScale + box.left*posXScale
        else
           spr.X = (180 + offsetCFJ - offsetX + obj.posXInt)*posXScale - box.right*posXScale
        end

        spr.Y = (203 + offsetY - obj.posYInt + box.top)*yScale + stageOffset
        spr.Width  = width
        spr.Height = height
    end
end

function draw_rect(canvas, left, top, right, bottom, color)
    canvas:getPen():setColor(color)
    canvas:getPen():setWidth(1)
    canvas:line(left,top, right,top)
    canvas:line(left,top, left,bottom)
    canvas:line(right,top, right,bottom+1)
    canvas:line(left,bottom, right,bottom)
end

-- Displays the hitboxes in the window
function display_hitboxes()
    -- DO THE JUMP
    stageID = readByte(game.base + game.stageID)
    if stageID &lt; 0 or stageID &gt; 10 then return end
    if mode &lt; CFJ_US then
        stageOffset = stageOffsets.CVS2[stageID+1]
        posXScale = emuScreen.Width / 358.0
    else
        stageOffset = stageOffsets.CFJ[stageID+1]
        posXScale = emuScreen.Width / 384.0
    end

    -- Canvas offsets
    local offsetX = readShort(game.base+game.cameraX, true)--readShort(game.base+game.cameraX, false)
    local offsetY = readShort(game.base+game.cameraY, true)--441 - shit

    -- Draw it in the emulator
    emuScreen.beginUpdate()
    xScale = emuScreen.Width / 384.0
    yScale = emuScreen.Height / 224.0

    -- Just get the data for every damn object.
    for i=1,count(objects) do
        local obj = objects[i]
        get_obj_data(nil, obj)

        -- Only if the object is active
        if isActive(obj) then
            -- Draw the axis
            obj.axisSpr.Visible = true
            obj.axisSpr.X = ((180 + offsetCFJ - offsetX + obj.posXInt)*posXScale) - 9*xScale + 1
            obj.axisSpr.Y = (203 + offsetY - obj.posYInt - 8)*yScale + stageOffset
            obj.axisSpr.Width  = math.ceil(axisSize.width*posXScale)
            obj.axisSpr.Height = math.ceil(axisSize.height*yScale)

            local boxes = obj[HITBOX_VULNERABILITY]
            local clsnCount = count(boxes)

            -- Destroy the vulnerability boxes
            -- Draw the damn vulnerability boxes
            for i=0,2 do
                obj.boxSprites[HITBOX_VULNERABILITY][i].Visible = false
            end
            for i=0,clsnCount-1 do
                box = boxes[i]
                if box ~= nil and next(box) ~= nil then
                    obj.boxSprites[HITBOX_VULNERABILITY][i].Visible = true
                    draw_hitbox(obj.boxSprites[HITBOX_VULNERABILITY][i], obj, box, offsetX, offsetY, 0.25)
                end
            end

            -- Draw the damn push box
            box = obj[HITBOX_PUSH]
            if box ~= nil then
                obj.boxSprites[HITBOX_PUSH].Visible = true
                draw_hitbox(obj.boxSprites[HITBOX_PUSH], obj, box, offsetX, offsetY, 0.25)
            else
                obj.boxSprites[HITBOX_PUSH].Visible = false
            end

            -- Draw the damn attack box
            box = obj[HITBOX_ATTACK]
            if box ~= nil then
                draw_hitbox(obj.boxSprites[HITBOX_ATTACK], obj, box, offsetX, offsetY, 0.5)
            else
                obj.boxSprites[HITBOX_ATTACK].Visible = false
            end
        else
            obj.axisSpr.Visible = false
            for i=0,2 do
                obj.boxSprites[HITBOX_VULNERABILITY][i].Visible = false
            end
            obj.boxSprites[HITBOX_ATTACK].Visible = false
            obj.boxSprites[HITBOX_PUSH].Visible = false
        end
    end

    -- End the update
    emuScreen.endUpdate()

    return clsnStr
end

-- If checked, will scale horizontal CLSNs
function scale_clsns(sender)
    if checkbox_getState(sender) == 0 then
        scaleCLSN = false
    else
        scaleCLSN = true
    end
end

-- Sets logging to the opposite value
function set_logging(sender)
    if sender:getState() == 0 then
        -- Close the log file if it's open.
        if logFile ~= nil then
            logFile:close()
            logFile = nil
        end
        if crackHeadTimer.OnTimer ~= nil then
            timer_onTimer(crackHeadTimer, nil)
        end
        logAnimations = false
    end
    if sender:getState() == 1 then
        -- Open the log file for writing.
        logFile = assert(io.open(ANIM_LOG_NAME, "w"))
        logAnimations = true
    end
end
