-- Game watchdog timer tick
function check_name(sender)
    if addressList == nil then return end
    -- Kawaks game detection
    gameName = addressList.getMemoryRecordByDescription("Kawaks Selected Game").getValue()
    if gameName ~= nil and string.len(gameName) &gt; 2 then
      if gameName:match("Street Fighter Alpha 3") or gameName:match("Street Fighter Zero 3") then --and not sfa3Mode then
        sfa3Mode = true
        game.vulnStart = 0x90
        game.atckStart = 0xA0
        game.numProj = 24
        game.posX    = 0x10
        game.posXFlt = 0x12
        game.posY    = 0x14
        game.posYFlt = 0x16
        game.velX    = 0x40
        game.velXFlt = 0x42
        game.velY    = 0x44
        game.velYFlt = 0x46
        game.offset  = 0x34
        game.actkID  = 0x09
        game.screenLeft = 0xFF8290
        game.swapWord = true
        game.activeOff = 0x06
        if getOpenedProcessID() ~= nil then
          onOpenProcess(getOpenedProcessID())
        end
      elseif gameName:match("Vampire Savior") or gameName:match("Vampire Hunter 2") then --and sfa3Mode then
        sfa3Mode = false
        game.vulnStart = 0x80
        game.atckStart = 0x8C
        game.numProj = 32
        game.posX    = 0x10
        game.posXFlt = 0x12
        game.posY    = 0x14
        game.posYFlt = 0x16
        game.velX    = 0x40
        game.velXFlt = 0x42
        game.velY    = 0x44
        game.velYFlt = 0x46
        game.offset  = 0x00
        game.atckId  = 0x0A
        game.screenLeft = 0xFF8290
        game.swapWord = true
        game.activeOff = 0x06
        if getOpenedProcessID() ~= nil then
          onOpenProcess(getOpenedProcessID())
        end
      else
        sfa3Mode = false
      end
    else
      gameName = addressList.getMemoryRecordByDescription("PCSX2 Game ID").getValue()
      if gameName ~= nil and string.len(gameName) &gt; 2 then
        if gameName:match("SLPM_659.98") then
            sfa3Mode = false
