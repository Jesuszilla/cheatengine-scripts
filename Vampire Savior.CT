-- Updates the velocity boxes
function update_data(Thread, form)
    if getOpenedProcessID() &gt; 0 then
        while not terminateThread do
            if form == nil then return end
            -- Initialize the list of objects
            if not dataInitialized then
                check_name(form)
                local objList = form.findComponentByName("ObjectListBox")
                initialize_data(objList)
                --print("WHAT")
            else
                -- Update the selected object's data
                synchronize(check_proj, form)
                synchronize(update_obj_data, form)
                display_hitboxes()
                --print("DO I GET HERE")
            end
            nextFrame = get_frames_drawn()
            while nextFrame == currFrame do nextFrame = get_frames_drawn() end
            currFrame = nextFrame
        end
    end
end

-- Updates P1's data and puts them in the proper boxes
function update_obj_data(mainForm)
    if selectedObj == nil or not dataInitialized then return end

    --print(string.format("selectedObj.addr = 0x%X", selectedObj.addr))
    --print(string.format("game.base = 0x%X", game.base))

    -- Address list &amp; form components
    local xVelBox = mainForm.findComponentByName("CEEdit1")
    local yVelBox = mainForm.findComponentByName("CEEdit2")
    local deltaXVelBox = mainForm.findComponentByName("CEEdit3")
    local deltaYVelBox = mainForm.findComponentByName("CEEdit4")
    local xPosBox = mainForm.findComponentByName("CEEdit5")
    local yPosBox = mainForm.findComponentByName("CEEdit6")

    -- Previous velocities
    prevVelX = tonumber(xVelBox.Text)
    prevVelY = tonumber(yVelBox.Text)

    --print(string.format("game.velX = 0x%02x", game.velX))

    -- Update the velocities
    --print(string.format("velXInt = 0x%X", game.base + obj.addr + game.velX))
    local velXInt = selectedObj.velXInt
    local velYInt = selectedObj.velYInt
    velX = selectedObj.velX
    velY = selectedObj.velY

    -- Update the positions
    local posXInt = selectedObj.posXInt
    local posYInt = selectedObj.posYInt
    posX = selectedObj.posX
    posY = selectedObj.posY

    -- Deltas
    deltaVelX = velX - prevVelX
    deltaVelY = velY - prevVelY

    -- Update the boxes
    xVelBox.Text = velX
    yVelBox.Text = velY
    if not (velX == prevVelX) then
       deltaXVelBox.Text = velX - prevVelX
    end
    if not (velY == prevVelY) then
       deltaYVelBox.Text = velY - prevVelY
    end
    xPosBox.Text = posX
    yPosBox.Text = posY
end

function start_timer(sender)
    local owner = sender.getOwner()
    local tb = owner.findComponentByName("HBVActive")

    -- Start the thread for getting data
    terminateThread = false
    createNativeThread(update_data, owner)
end

-- Sets logging to the opposite value
function set_logging(sender)
    if sender:getState() == 0 then
        -- Close the log file if it's open.
        if logFile ~= nil then
            logFile:close()
            logFile = nil
        end
        logAnimations = false
    end
    if sender:getState() == 1 then
        -- Open the log file for writing.
        logFile = assert(io.open(ANIM_LOG_NAME, "w"))
        logAnimations = true
    end
end

-- Gets the specified hitbox coordinates
function get_obj_data(sender, obj)
    if obj == nil or game.base == 0 then return end

    -- Are we copying CLSN?
    local copyingCLSN = false
    local copyingPush = false
    if sender ~= nil then
        if sender.Caption ~= nil then
           if sender.Caption:match("Copy CLSNs") then
              copyingCLSN = true
           elseif sender.Caption:match("Copy Push") then
              copyingPush = true
           end
        end
    end

    --print(string.format("headBoxTable = 0x%x", game.base + selectedObj.addr + game.vulnStart))
    local baseAddr = game.base + obj.addr
    local headBoxTable = readInteger(baseAddr + game.vulnStart + 0x0)
    local bodyBoxTable = readInteger(baseAddr + game.vulnStart + 0x4)
    local legsBoxTable = readInteger(baseAddr + game.vulnStart + 0x8)
    local pushBoxTable = readInteger(baseAddr + game.vulnStart + (sfa3Mode and 0xC or 0x10))
    local atckBoxTable = readInteger(baseAddr + game.atckStart)
    local animPtr = readInteger(baseAddr + game.anim)

    --if copyingCLSN then print(string.format("baseAddr + game.anim = 0x%X, animPtr raw = 0x%X", baseAddr + game.anim, animPtr)) end

    if not sfa3Mode and game.base == emuBase.demul then
       headBoxTable = (headBoxTable &amp; vcAddrMask)
       bodyBoxTable = (bodyBoxTable &amp; vcAddrMask)
       legsBoxTable = (legsBoxTable &amp; vcAddrMask)
       pushBoxTable = (pushBoxTable &amp; vcAddrMask)
       atckBoxTable = (atckBoxTable &amp; vcAddrMask)
       animPtr = (animPtr &amp; vcAddrMask)
       --print(string.format("legsBoxTable = 0x%X", legsBoxTable))
    end

    -- Update the velocities
    --print(string.format("velXInt = 0x%X", game.base + obj.addr + game.velX))
    local velXInt = readShort(game.base + obj.addr + game.velX, true)
    local velYInt = readShort(game.base + obj.addr + game.velY, true)
    velX = velXInt + (readShort(game.base + obj.addr + game.velXFlt) / 65536.0)
    velY = - (velYInt + (readShort(game.base + obj.addr + game.velYFlt) / 65536.0))
    obj.velXInt = velXInt
    obj.velX = velX
    obj.velYInt = velYInt
    obj.velY = velY

    -- Update the positions
    local posXInt = readShort(game.base + obj.addr + game.posX, true)
    local posYInt = readShort(game.base + obj.addr + game.posY, true)
    posX = posXInt + (readShort(game.base + obj.addr + game.posXFlt, false) / 65536.0)
    posY = - (posYInt + (readShort(game.base + obj.addr + game.posYFlt, false) / 65536.0))
    obj.posXInt = posXInt
    obj.posX = posX
    obj.posYInt = posYInt
    obj.posY = posY

    --if is_active(obj) then
        -- Head box
        --print(string.format("headBoxTable = 0x%X", headBoxTable))
        local headBoxOffset, hbOff, bodyBoxOffset, bbOff, legsBoxOffset, lbOff, pushBoxOffset, pbOff
        if base == boxBase.pcsx2 or base == boxBase.demul then
          headBoxOffset = readByte(baseAddr + game.offset + 0x94)
          hbOff = bShl(headBoxOffset, game.shift)
          --print("headBoxOffset = " .. headBoxOffset)

          -- Body box
          bodyBoxOffset = readByte(baseAddr + game.offset + 0x95)
          bbOff = bShl(bodyBoxOffset, game.shift)
          --print("bodyBoxOffset = " .. bodyBoxOffset)

          -- Legs box
          legsBoxOffset = readByte(baseAddr + game.offset + 0x96)
          lbOff = bShl(legsBoxOffset, game.shift)
          --print("legsBoxOffset = " .. legsBoxOffset)

          -- Push box
          pushBoxOffset = readByte(baseAddr + game.offset + 0x97)
          pbOff = bShl(pushBoxOffset, game.shift)
        else
          headBoxOffset = readByte(baseAddr + game.offset + ((game.swapWord and not sfa3Mode) and 0x95 or 0x95))
          hbOff = bShl(headBoxOffset, game.shift)
          --print("headBoxOffset = " .. headBoxOffset)

          -- Body box
          bodyBoxOffset = readByte(baseAddr + game.offset + ((game.swapWord and not sfa3Mode) and 0x94 or 0x97))
          bbOff = bShl(bodyBoxOffset, game.shift)
          --print("bodyBoxOffset = " .. bodyBoxOffset)

          -- Legs box
          legsBoxOffset = readByte(baseAddr + game.offset + ((game.swapWord and not sfa3Mode) and 0x97 or 0x94))
          lbOff = bShl(legsBoxOffset, game.shift)
          --print("legsBoxOffset = " .. legsBoxOffset)

          -- Push box
          pushBoxOffset = readByte(baseAddr + game.offset + ((game.swapWord and not sfa3Mode) and 0x96 or 0x96))
          pbOff = bShl(pushBoxOffset, game.shift)
        end
        --print("legsBoxOffset = " .. pushBoxOffset)

        -- Load the damn hitboxes
        obj[HITBOX_VULNERABILITY] = {}
        hitbox_load(obj, 0, HITBOX_VULNERABILITY, (game.swapWord and word_swap(headBoxTable) or headBoxTable) + hbOff)
        hitbox_load(obj, 1, HITBOX_VULNERABILITY, (game.swapWord and word_swap(bodyBoxTable) or bodyBoxTable) + bbOff)
        hitbox_load(obj, 2, HITBOX_VULNERABILITY, (game.swapWord and word_swap(legsBoxTable) or legsBoxTable) + lbOff)
        hitbox_load(obj, 0, HITBOX_PUSH, (game.swapWord and word_swap(pushBoxTable) or pushBoxTable) + pbOff)

        -- Load the damn attack box
        obj[HITBOX_ATTACK] = {}
        local atckBoxOffset = (game.swapWord and word_swap(animPtr) or animPtr) + game.atckID

        local abOff = readByte(base + atckBoxOffset)
        --print(string.format("atckBoxOffset = 0x%X", atckBoxOffset))
        --print(string.format("shit = 0x%X", base + atckBoxOffset))
        --print(string.format("abOff = 0x%X", abOff))
        --print(string.format("atckBoxTable = 0x%X", atckBoxTable))
        --print(string.format("abOff addr = 0x%X", base + atckBoxOffset - (sfa3Mode and 2 or -1)))
        if abOff then
            hitbox_load(obj, 0, HITBOX_ATTACK, (game.swapWord and word_swap(atckBoxTable) or atckBoxTable) + abOff*0x20)
        end

        -- Copy them to clipboard
        local clsn2 = convert_to_air(obj, HITBOX_VULNERABILITY)
        local clsn1 = convert_to_air(obj, HITBOX_ATTACK)

        if copyingCLSN then
            --print(string.format("atckBoxOffset = 0x%X, abOff = 0x%X", atckBoxOffset, abOff))
            if clsn1 == "" then
                writeToClipboard(clsn2)
            else
                writeToClipboard(clsn2 .. "\n" .. clsn1)
            end
        elseif copyingPush then
            local clsn0 = convert_to_air(obj, HITBOX_PUSH)
            writeToClipboard(clsn0)
        end

        -- Logging animations?
        if logAnimations and obj == selectedObj then
            local frameTimingOffset = (game.swapWord and word_swap(animPtr) or animPtr) + game.frameTiming
            local frameTiming = readByte(base + frameTimingOffset)
            selectedObj[SPRITE_NO] = animPtr + base
            selectedObj[FRAME_COUNT] = frameTiming
            if lastObj == nil or
            selectedObj[SPRITE_NO] ~= lastObj[SPRITE_NO] or
            selectedObj[FRAME_COUNT] ~= lastObj[FRAME_COUNT] or
            not table_eq(selectedObj[HITBOX_PUSH], lastObj[HITBOX_PUSH]) or
            not table_eq(selectedObj[HITBOX_ATTACK], lastObj[HITBOX_ATTACK]) or
            not table_eq(selectedObj[HITBOX_VULNERABILITY], lastObj[HITBOX_VULNERABILITY]) then
                if lastObj ~= nil then
                    local lastClsn2 = convert_to_air(lastObj, HITBOX_VULNERABILITY)
                    local lastClsn1 = convert_to_air(lastObj, HITBOX_ATTACK)
                    local lastClsn0 = convert_to_air(lastObj, HITBOX_PUSH)
                    local lastSprite = lastObj[SPRITE_NO]
                    local lastFrameCount = lastObj[FRAME_COUNT]

                    if lastSprite then
                        if lastFrameCount and lastFrameCount &gt; 0 then
                            -- Print the current hitboxes.
                            if lastClsn1 == "" then
                                logFile:write(lastClsn2 .. "\n")
                            else
                                logFile:write(lastClsn2 .. "\n" .. lastClsn1 .. "\n")
                            end

                            -- Print the current spriteNo and timing
                            logFile:write(string.format("0,%d, 0,0, %d\n", lastSprite, lastFrameCount))
                        end
                    end
                end

                -- Reset the data
                lastObj = deepcopy(selectedObj)
            end
        end
    --end
end

-- Gets the pushbox data
function get_obj_push(sender)
    if selectedObj == nil then return end
    get_obj_data(sender, selectedObj)
end

-- Converts hitboxes to .AIR syntax
function convert_to_air(obj, boxType)
    local boxes = obj[boxType]
    local clsnBase = ""
    local clsnStr = ""
    local clsnCount = count(boxes)

    -- If there are no CLSNs, return an empty string
    if clsnCount == 0 then
        return ""
    end

    -- There can be only one attack box at any given moment
    if boxType == HITBOX_ATTACK or boxType == HITBOX_PUSH then
        clsnCount = 1
    end

    clsnBase = string.format("Clsn%d", boxType)
    clsnsStr = ""
    boxNo = 0
    for i=0,clsnCount-1 do
        local box
        if boxType == HITBOX_ATTACK or boxType == HITBOX_PUSH then box = boxes else box = boxes[i] end
        if box ~= nil then
            local coords = ""
            if box.left == 0 and box.top == 0 and box.right == 0 and box.bottom == 0 then
                clsnCount = clsnCount - 1
            else
                coords = string.format("%d,%d,%d,%d", box.left, box.top, box.right, box.bottom)
                clsnsStr = clsnsStr .. string.format("\n  %s[%d] = ", clsnBase, boxNo) .. coords
                boxNo = boxNo + 1
            end
        end
    end

    if clsnCount == 0 then
        return ""
    else
        clsnStr = clsnBase .. ": " .. clsnCount
        return clsnStr .. clsnsStr
    end
end

-- Swaps 16-bit words.
function word_swap(input)
    local front = bAnd(input, 0xFFFF)
    local back = bAnd(input, 0xFFFF0000)
    local newBack = bShl(front, 16)
    local newFront = bShr(back, 16)
    return bOr(newFront, newBack)
end

