function hitbox_load(obj, i, boxType, addr)
    local addrBase = base + addr
	local hval    = readShort(addrBase + 0, true)
	local vval    = readShort(addrBase + 2, true)
	local hrad    = readShort(addrBase + 4, true)
	local vrad    = readShort(addrBase + 6, true)
    local d = string.format("hval = 0x%X, vval = 0x%X, hrad = 0x%X, vrad = 0x%X", hval,vval,hrad,vrad)
    --print(string.format("addrBase = 0x%X", addrBase))
    --print(string.format("addrDec = 0x%X", tonumber(addrDec) + 2))
    --print(d)

	local left   = - (hval + hrad)
	local top    = - (vval + vrad)
	local right  = - (hval - hrad)
	local bottom = - (vval - vrad)
    --print(string.format("L,T,R,B = %d,%d,%d,%d", left, top, right, bottom))

	if boxType == HITBOX_VULNERABILITY then
		obj[HITBOX_VULNERABILITY][i] = {
			left    = left,
			right   = right,
			bottom  = bottom,
			top     = top,
			hval    = hval,
			vval    = vval,
			boxType = boxType
		}
	else
		obj[boxType] = {
			left    = left,
			right   = right,
			bottom  = bottom,
			top     = top,
			hval    = hval,
			vval    = vval,
			boxType = boxType
		}
	end
end

-- Populate the listbox
function initialize_data(listbox)
    dataInitialized = true
    local listItems = {}
    listbox.items.clear()
    objects = {player1, player2}
    --print(string.format("p1.addr = 0x%X, p2.addr = 0x%X, game.base = 0x%X", player1.addr, player2.addr, game.base))
    -- Allocate the object list with the number of projectiles
    for i=0,game.numProj do
        local n = "Projectile " .. i
        objects[i+3] = {name = string.format("Projectile %d", i),
                        addr = game.projStart+(game.projOffset*i),
                        boxSprites = {}
    }
    end
    if not hitboxSpritesLoaded then
      for _,obj in pairs(objects) do
          obj.axisSpr = emuScreen.createSprite(texC)
          axisSize.width = obj.axisSpr.Width
          axisSize.height = obj.axisSpr.Height
          obj.boxSprites[HITBOX_VULNERABILITY] = {}
          for i=0,2 do
              obj.boxSprites[HITBOX_VULNERABILITY][i] = emuScreen.createSprite(texV)
          end
          obj.boxSprites[HITBOX_PUSH] = emuScreen.createSprite(texP)
          obj.boxSprites[HITBOX_ATTACK] = emuScreen.createSprite(texA)
      end
      hitboxSpritesLoaded = true
    end
    for k,v in spairs(objects, compare_numeric_names) do
        listbox.items.add(objects[k]['name'])
    end
    selectedObj = objects[0]
end

function set_selected_obj(sender, user)
    for _,v in pairs(objects) do
        if v['name'] == sender.Items[sender.getItemIndex()] then
            selectedObj = v
            break
        end
    end
end

-- Displays the hitboxes in the window
function display_hitboxes()
    -- Canvas offsets
    local offsetX = readShort(game.base + game.screenLeft, true)
    local offsetY = readShort(game.base + game.screenLeft - 0x02, true)

    -- Draw it in the emulator
    emuScreen.beginUpdate()
    xScale = emuScreen.Width / 384.0
    yScale = emuScreen.Height / 224.0

    -- Just get the data for every damn object.
    for i=1,count(objects) do
        local obj = objects[i]
        get_obj_data(nil, obj)

        -- Only if the object is active
        if is_active(obj) then
            --print("AAAAAAAAAAGH")
            -- Draw the axis
            obj.axisSpr.Visible = true
            obj.axisSpr.X = (obj.posXInt - offsetX - 1)*xScale
            obj.axisSpr.Y = (emuScreen.Height + obj.posYInt + offsetY)*yScale
            obj.axisSpr.Width  = math.ceil(axisSize.width*xScale)
            obj.axisSpr.Height = math.ceil(axisSize.height*yScale)

            local boxes = obj[HITBOX_VULNERABILITY]
            local clsnCount = count(boxes)

            -- Destroy the vulnerability boxes
            -- Draw the damn vulnerability boxes
            for i=0,2 do
                obj.boxSprites[HITBOX_VULNERABILITY][i].Visible = false
            end
            for i=0,clsnCount-1 do
                box = boxes[i]
                if box ~= nil and next(box) ~= nil then
                    obj.boxSprites[HITBOX_VULNERABILITY][i].Visible = true
                    draw_hitbox(obj.boxSprites[HITBOX_VULNERABILITY][i], obj, box, offsetX, offsetY, 0.25)
                end
            end

            -- Draw the damn push box
            box = obj[HITBOX_PUSH]
            if box ~= nil then
                obj.boxSprites[HITBOX_PUSH].Visible = true
                draw_hitbox(obj.boxSprites[HITBOX_PUSH], obj, box, offsetX, offsetY, 0.25)
            else
                obj.boxSprites[HITBOX_PUSH].Visible = false
            end

            -- Draw the damn attack box
            box = obj[HITBOX_ATTACK]
            if box ~= nil then
                draw_hitbox(obj.boxSprites[HITBOX_ATTACK], obj, box, offsetX, offsetY, 0.5)
            else
                obj.boxSprites[HITBOX_ATTACK].Visible = false
            end
        else
            obj.axisSpr.Visible = false
            for i=0,2 do
                obj.boxSprites[HITBOX_VULNERABILITY][i].Visible = false
            end
            obj.boxSprites[HITBOX_ATTACK].Visible = false
            obj.boxSprites[HITBOX_PUSH].Visible = false
        end
    end

    -- End the update
    emuScreen.endUpdate()

    --return clsnStr
end

-- Draws in the actual game window
function draw_hitbox(spr, obj, box, offsetX, offsetY, alpha)
    if box == nil then return end
    if box.left == nil or box.right == nil or box.top == nil or box.bottom == nil then return end
    if spr ~= nil then
        flip = readByte(game.base + obj.addr + game.flip)
        width = math.ceil((box.right - box.left + 1)*xScale)
        height = math.ceil((1 + math.abs(box.top - box.bottom))*yScale)
        -- Now do the sprite crap
        spr.Alphablend = alpha

        -- DO THE FLIP
        if flip == 1 then
           spr.X = (obj.posXInt - offsetX - 1 + box.left)*xScale
        else
           spr.X = (obj.posXInt - offsetX - 1 - box.right)*xScale
        end

        spr.Y = (emuScreen.Height + obj.posYInt + box.top + offsetY)*yScale
        spr.Width  = width
        spr.Height = height
    end
end

function get_frames_drawn()
    return readInteger(game.base + game.frameTimer)
end

function get_obj_boxes(sender)
    if selectedObj == nil then return end
    get_obj_data(sender, selectedObj)
end
