            game = pcsx2.games[gameName][readByte(gameIDAddr)]
            if getOpenedProcessID() ~= nil then
              onOpenProcess(getOpenedProcessID())
            end
        end
      else
          gameName = addressList.getMemoryRecordByDescription("DEmul Game ID").getValue()
          if gameName ~= nul and string.len(gameName) &gt; 2 then
              if gameName:match("VAMPIRE CHRONICLE") then
                  sfa3Mode = false
                  player1.addr = 0x00600400
                  player2.addr = 0x00600800
                  proj0.addr = 0x00600900
                  game.activeOff = 0x07
                  game.frameTimer = 0x006000B4
                  game.posX = 0x12
                  game.posXInt = 0x10
                  game.posY = 0x16
                  game.posYInt = 0x14
                  game.velX = 0x42
                  game.velXFlt = 0x40
                  game.velY = 0x46
                  game.velYFlt = 0x44
                  game.screenLeft = 0x00600026
                  game.swapWord = false
                  game.atckID = 0x0A
